{
  "name": "jadeview",
  "displayName": "jadeview",
  "description": "Render html documents as Jade for easier reading",
  "version": "0.0.1",
  "publisher": "vivainio",
  "repository": {
      "type": "git",
      "url": "https://github.com/vivainio/jadeview"
  },
  "author": "Ville M. Vainio <vivainio@gmail.com>",

  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.jadeView"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.jadeView",
        "title": "Jade View"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.7.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "html2jade": "^0.8.4",
    "tmp": "0.0.28"
  }
}
